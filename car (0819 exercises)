public class Car {
	private String model;
	private int year;
	public Car(String model, int year) {
		this.model = model;
		this.year = year;
	}
	public String setCarModel(String model){
		this.model = model;
		return model;
	}
	public String getCarModel(){
		return model;
	}
	public int setCarYear(int year){
		this.year = year;
		return year;
	}
	public int getCarYear(){
		return year;
	}
	public String toString(){
		return CarModel: "+ model +"      CarYear: "+ year +";
	}
  
  public static void main(String[] args) {
		Car car1 = new Car("Uno", 1995);
		Car car2 = new Car("Palio", 2008);
		
		car1.setCarModel("Uno")
		System.out.println(car1);
		System.out.println(car1.getCarYear());

		car2.setCarYear(2008)
		System.out.println(car2);
		System.out.println(car2.getCarModel());
	}
}
